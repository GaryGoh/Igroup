<div class="field">
  <%= f.attribute_fields do |a| %>
      <%= a.attribute_select({associations: [:category]}, {:class => "browser-default col s3"}) %>
  <% end %>
  <%= f.predicate_select({only: [:cont]}, {:class => "browser-default col s3"}) %>
  <%= f.value_fields do |v| %>
      <%= v.text_field :value, :class => "col s3" %>
  <% end %>
  <%= link_to "remove", '#', class: "remove_fields col s3 btn" %>
</div>
